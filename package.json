{
  "name": "williamravel",
  "version": "1.0.0",
  "type": "module",
  "description": "Site internet williamravel",
  "main": "index.js",
  "scripts": {
    "cleanup:local": "rm -rf _site dist",
    "serve": "eleventy --serve",
    "build": "NODE_ENV=production concurrently \"vite build\" \"wait-on dist/styles.css && eleventy --serve\"",
    "dev": "concurrently \"vite build --watch\" \"wait-on dist/styles.css && eleventy --serve\"",
    "build:netlify": "npm run build && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willahh/williamravel-site.git"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@tailwindcss/postcss": "^4.0.7",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.7",
    "vite": "^6.1.1",
    "wait-on": "^8.0.2"
  }
}
